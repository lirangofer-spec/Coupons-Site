<script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/2.0.0/jquery.min.js"></script>

<h1>Add <%= title %></h1>
<h2>Welcome to add <%= title %> page</h2>
<form action = "<%= action %>" method ="post" enctype="multipart/form-data" />
    Coupon id: <input type ="text" name ="id" placeholder="Enter coupon id" value = "<%= item.id %>" required autofocus/>
    </br>
    Coupon Name: <input type ="text" name ="name" placeholder="Enter coupon name" value="<%= item.name %>" required autofocus/>
    </br>
    Coupon Category:
    <select name="category"  id = "category" value="<%= item.category %>" required autofocus>
    </select>
    </br>
    Dynamic Business Type (from server):
    <select name ="businessType" value="<%= item.businessType %>" required autofocus>
    <option>car</option>
    </select>
    </br>
    <!-- <input type ="text" name ="imagefilename" placeholder="Enter image name"  value="<%= item.name %>" required autofocus/> -->
    </br>
    Type a description: <input type ="text" name ="description" placeholder="Type a description"  value="<%= item.description %>" required autofocus/>
    </br>
    <div>
        <label>
            Image
            <input type="file" accept="image/*" name="postpic" />
        </label>
    </div>
    </br>
    <input type = "submit" >
    </form>


  
<script src='/javascripts/getcategory.js' type='text/javascript' > </script> 







    